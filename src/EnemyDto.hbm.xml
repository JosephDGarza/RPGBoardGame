<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dto.EnemyDto" table="enemies"
		schema="rpgboard">


		<id name="id">
			<column name="id" sql-type="int(255)"></column>
		</id>
		<property name="imgurl">
			<column name="imgurl" sql-type="varchar(255)"></column>
		</property>
		
		<property name="name">
			<column name="name" sql-type="varchar(255)"></column>
			</property>
		<property name="weakness">
			<column name="weakness" sql-type="varchar(255)" length="10"></column>
		</property>
		<property name="str">
			<column name="str" sql-type="int(255)" length="255"></column>
		</property>
		<property name="dodge">
			<column name="dodge" sql-type="int(255)" length="255"></column>
		</property>
		<property name="mana">
			<column name="mana" sql-type="int(255)"></column>
		</property>
		<property name="hp">
			<column name="hp" sql-type="int(255)"></column>
		</property>
		<property name="currentHp">
			<column name="currentHp" sql-type="int(255)"></column>
		</property>
				<property name="miss">
			<column name="miss" sql-type="int(255)"></column>
		</property>
				<property name="missScript">
			<column name="missScript" sql-type="varchar(255)"></column>
		</property>
				<property name="attack">
			<column name="attack" sql-type="varchar(255)"></column>
		</property>

		
		
		
	</class>
</hibernate-mapping>	